<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pooja.foodex">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" />
        <activity android:name=".Register" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CanteenActivity" />
        <activity android:name=".Home_Canteen" />
        <activity android:name=".Menu_Page" />
        <activity android:name=".Add_Dish" />
        <activity android:name=".Canteen_Page" />
        <activity android:name=".MenuForStudent" />
        <activity android:name=".PendingOrder" />
        <activity android:name=".StudentPendingOrder" />
        <activity android:name=".StudentAccount" />
        <activity android:name=".CanteenAccount" />
    </application>

</manifest>